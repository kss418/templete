    "            for(int i = 0;i < q.size();i++){",
    "                if(l[i] >= r[i]) continue;",
    "                ll mid = (l[i] + r[i] + 1) >> 1ll;",
    "                arr[mid].push_back(i); flag = 1;",
    "            }",
    "            if(!flag) break;",
    "",
    "            ll num = 0;",
    "            for(ll mid = lo;mid <= hi;mid++){",
    "                for(auto& idx : arr[mid]){",
    "                    if(f(mid, idx)) l[idx] = mid;",
    "                    else r[idx] = mid - 1;",
    "                }",
    "            }",
    "        }",
    "    }",
    "};"
