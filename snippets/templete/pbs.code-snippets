    "",
    "    void clear(){",
    "        for(ll i = lo;i <= hi;i++) arr[i].clear();",
    "    }",
    "",
    "    bool f(ll cur, ll idx){ // 결정 함수",
    "        query& now = q[idx];",
    "",
    "        return 0;",
    "    }",
    "",
    "    void init(){",
    "        arr.resize(hi + 1);",
